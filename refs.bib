pages = {111},
pmid = {17697325},
title = {{Detection of knockdown resistance ({\textless}i{\textgreater}kdr{\textless}/i{\textgreater}) mutations in {\textless}i{\textgreater}Anopheles gambiae{\textless}/i{\textgreater}: a comparison of two new high-throughput assays with existing methods.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1971715{\&}tool=pmcentrez{\&}rendertype=abstract},
