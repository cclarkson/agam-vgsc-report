isbn = {1095-9939 (Electronic)$\backslash$r0048-3575 (Linking)},
issn = {00483575},
journal = {Pesticide Biochemistry and Physiology},
keywords = {Knockdown resistance,Pyrethroid resistance,Sodium channel,Sodium channel mutations},
number = {3},
